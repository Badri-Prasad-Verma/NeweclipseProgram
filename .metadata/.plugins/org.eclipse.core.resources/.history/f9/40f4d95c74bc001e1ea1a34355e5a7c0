package app_java_1;

import java.util.Arrays;

public class A {
	public static void main(String[] args) {
		String str="matlab";
		boolean vowelCheck=isVowelPresentOrNot(str);
		System.out.println(str);
		System.out.println(vowelCheck);
	}

	private static boolean isVowelPresentOrNot(String str) {
		boolean matches = str.toLowerCase().matches("aeiou");
		return matches;
	}
}
